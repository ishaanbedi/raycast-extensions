{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "http-status-code-lookup",
  "title": "HTTP Status Code Lookup",
  "description": "Quick reference for HTTP status codes with descriptions and usage examples",
  "icon": "command-icon.png",
  "author": "ishaanbedi",
  "categories": [
    "Developer Tools",
    "Web",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "HTTP Status Code Lookup",
      "description": "Search and view details of HTTP status codes",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}